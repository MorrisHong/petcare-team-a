<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.petcare.web.mapper.LoginMapper">
    <select id="findCorrectUser" resultType="com.petcare.web.domain.MemberVO">
        select *
        from user
        where user_id = #{userId}
          and user_pw = #{userPw}
    </select>
</mapper>